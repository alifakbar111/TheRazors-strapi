{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "private": false,
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "featured": {
      "type": "boolean"
    }
  }
}
